<h1> Sign up </h1>

<input type = "hidden" value = "<%= form_authenticity_token %>">

<form action="<%= users_url %>" method="POST">
    <label for="email"> email</label>
    <input type="text" id="email" name="user[email]" value="<%= @user.email %>">
    <br>
    <label for="password">Password</label>
    <input type= "password" id="password" name="user[password]" value="<%= @user.email %>">

    <button> Sign up</button>

</form>